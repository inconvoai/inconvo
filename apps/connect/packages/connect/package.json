{
  "name": "@ten-dev/inconvo",
  "version": "0.0.71",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "pull": "npx pull-schema",
    "test:ecommerce": "jest",
    "dev": "tsup --watch"
  },
  "bin": {
    "pull-schema": "dist/pullSchema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ten-dev/inconvo-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    "./express": {
      "require": "./dist/src/express/index.js",
      "import": "./dist/src/express/index.js"
    },
    "./next": {
      "require": "./dist/src/next/index.js",
      "import": "./dist/src/next/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "UNLICENSED",
  "peerDependencies": {
    "express": "^4.21.1"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "fast-levenshtein": "^3.0.0",
    "mysql2": "^3.15.1",
    "next": "^15.5.4",
    "pg": "^8.13.1",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "ts-morph": "^27.0.0",
    "typescript": "^5.9.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5"
  },
  "tsup": {
    "entry": [
      "src/express/index.ts",
      "src/next/index.ts",
      "src/server/index.ts",
      "drizzle.config.ts",
      "src/drizzleOutputParser.ts"
    ],
    "minify": true,
    "bundle": true,
    "splitting": false,
    "clean": true,
    "dts": true,
    "css": true,
    "outDir": "dist",
    "onSuccess": "cp src/pullSchema.js dist/pullSchema.js"
  }
}
