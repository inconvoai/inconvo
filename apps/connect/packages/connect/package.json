{
  "name": "@ten-dev/inconvo",
  "version": "0.0.68",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "pull": "npx pull-schema",
    "test:ecommerce": "jest",
    "dev": "tsup --watch"
  },
  "bin": {
    "pull-schema": "dist/pullSchema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ten-dev/inconvo-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    "./express": {
      "require": "./dist/src/express/index.js",
      "import": "./dist/src/express/index.js"
    },
    "./next": {
      "require": "./dist/src/next/index.js",
      "import": "./dist/src/next/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "UNLICENSED",
  "peerDependencies": {
    "express": "^4.21.1"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.2",
    "drizzle-orm": "^0.44.2",
    "fast-levenshtein": "^3.0.0",
    "mysql2": "^3.14.1",
    "next": "^15.3.4",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3"
  },
  "tsup": {
    "entry": [
      "src/express/index.ts",
      "src/next/index.ts",
      "src/server/index.ts",
      "drizzle.config.ts",
      "src/drizzleOutputParser.ts"
    ],
    "minify": true,
    "bundle": true,
    "splitting": false,
    "clean": true,
    "dts": true,
    "css": true,
    "outDir": "dist",
    "onSuccess": "cp src/pullSchema.js dist/pullSchema.js"
  }
}
