generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["prismaSchemaFolder", "relationJoins"]
}

generator drizzle {
    provider = "drizzle-prisma-generator"
    output   = "../../../drizzle"
}

datasource db {
    provider = "postgresql"
    url      = env("INCONVO_DATABASE_URL")
}
