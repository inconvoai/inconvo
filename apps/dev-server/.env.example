# Inconvo Dev Server Configuration
# Copy this to .inconvo.env and fill in your values
# Or run `pnpm setup` to use the interactive wizard

# Telemetry Configuration (Optional)
# Set to "true" to disable all analytics telemetry
DISABLE_TELEMETRY=false
NEXT_PUBLIC_DISABLE_TELEMETRY=false

# PostHog Configuration (only if telemetry enabled)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Database Configuration
DATABASE_DIALECT=postgresql  # postgresql, mysql, or mssql
INCONVO_DATABASE_URL=postgresql://user:password@localhost:5432/database
INCONVO_DATABASE_SCHEMA=public  # Optional

# LLM Configuration
OPENAI_API_KEY=sk-...

# LangChain Configuration (for prompts)
LANGCHAIN_API_KEY=lsv2_pt_...
