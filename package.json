{
  "name": "inconvo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "dev": "next dev --turbopack",
    "dev:inngest": "concurrently \"npm run dev\" \"npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest\" --names \"next,inngest\" --prefix-colors \"cyan,magenta\"",
    "postinstall": "prisma generate && patch-package",
    "check": "eslint . && tsc --noEmit",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "eval": "vitest run --config ls.vitest.config.ts",
    "test:schema-validator": "vitest run --config validator.vitest.config.ts",
    "evals:e2e": "NODE_ENV=development dotenv -e .env npx tsx evals/e2e/inconvoAgent/index.ts",
    "evals:simulated": "NODE_ENV=development dotenv -e .env npx tsx evals/e2e/simulated/index.ts",
    "typecheck": "tsc --noEmit"
  },
