    "@mantine/notifications": "^8.3.8",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@propelauth/nextjs": "^0.3.16",
