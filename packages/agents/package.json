{
  "name": "@repo/agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./database": {
      "import": "./dist/database/index.js",
      "types": "./dist/database/index.d.ts"
    },
    "./inconvo": {
      "import": "./dist/inconvo/index.js",
      "types": "./dist/inconvo/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --color .",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@langchain/core": "1.1.6",
    "@langchain/langgraph": "1.0.7",
    "@langchain/langgraph-checkpoint-postgres": "1.0.0",
    "@langchain/openai": "1.2.0",
    "@repo/types": "workspace:*",
    "@ten-dev/inconvo": "workspace:*",
    "langchain": "1.2.1",
    "langsmith": "0.4.0",
    "yaml": "2.8.2",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "25.0.3",
    "eslint": "9.39.2",
    "tsup": "^8.5.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/database/index.ts",
      "src/inconvo/index.ts"
    ],
    "format": ["esm"],
    "dts": true,
    "clean": true,
    "outDir": "dist"
  }
}
