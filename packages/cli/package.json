{
  "name": "inconvo",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "description": "CLI for Inconvo development environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/inconvoai/inconvo.git",
    "directory": "packages/cli"
  },
  "type": "module",
  "bin": {
    "inconvo": "./bin/inconvo.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "bin",
    "assets",
    "demo-db-compose.yml",
    "demo-db-init.sql"
  ],
  "scripts": {
    "build": "node scripts/build-sandbox-bundle.js && tsc",
    "dev:local": "tsx src/index.ts dev",
    "lint": "eslint --color .",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "node scripts/resolve-catalog.js && pnpm build",
    "postpublish": "git checkout package.json"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@faker-js/faker": "^10.3.0",
    "commander": "^14.0.3",
    "kysely": "catalog:",
    "mysql2": "^3.17.2",
    "pg": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "esbuild": "^0.27.3",
    "eslint": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
