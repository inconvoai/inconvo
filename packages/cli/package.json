{
  "name": "@ten-dev/inconvo",
  "version": "0.1.0",
  "description": "CLI for Inconvo development environment",
  "type": "module",
  "bin": {
    "inconvo": "./bin/inconvo.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "bin",
    "docker-compose.yml"
  ],
  "scripts": {
    "build": "tsc",
    "dev:local": "tsx src/index.ts dev",
    "lint": "eslint --color .",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "node scripts/resolve-catalog.js && pnpm build",
    "postpublish": "git checkout package.json"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^13.1.0",
    "kysely": "catalog:",
    "mysql2": "^3.16.1",
    "pg": "catalog:",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "eslint": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
